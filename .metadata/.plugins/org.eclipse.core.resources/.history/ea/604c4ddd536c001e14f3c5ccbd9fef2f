package com.rafaelpino.workshopmongo.domain;

import java.util.Date;

public class Post {

	private String id;
	private Date date;
	private String title;
	private String body;
	
	public Post() {
	}
	
	public Post(String id, Date date, String title, String body) {
		super();
		this.id = id;
		this.date = date;
		this.title = title;
		this.body = body;
	}
	
	
	
	
}
